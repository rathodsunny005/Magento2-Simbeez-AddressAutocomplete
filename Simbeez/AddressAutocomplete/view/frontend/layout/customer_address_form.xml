<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="content">
            <block name="customer_address_autocomplete" ifconfig="simbeez_address/autocomplete/active"
                   template="Simbeez_AddressAutocomplete::address/autocomplete.phtml" cacheable="true" after="customer_address_edit">
                <arguments>
                    <argument name="view_model" xsi:type="object">Simbeez\AddressAutocomplete\ViewModel\Autocomplete</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
